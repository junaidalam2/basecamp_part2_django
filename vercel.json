{
    "version": 2,
    "builds": [
      {
        "src": "basecamp/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb", "runtime": "python3.12"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "basecamp/wsgi.py"
      }
    ]
  }
  